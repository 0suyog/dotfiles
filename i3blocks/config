[click]
full_text=Click me!
command=echo "Got clicked with button $button"
color=#F79494

# Update time every 5 seconds
[time]
command=date +%T
interval=5

[battery]
command=~/.config/i3blocks/scripts/batteryInfo.sh
interval=3
markup=pango

[wifi]
command=~/.config/i3blocks/scripts/wifi_status.sh
interval=3
markup=pango

[sound]
command=~/.config/i3blocks/scripts/sound.sh
interval=once
signal=10
markup=pango
